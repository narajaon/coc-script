{
  "name": "coc-script",
  "version": "1.0.3",
  "description": "script extension for coc.nvim",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack .",
    "prepare": "npx npm-run-all clean build"
  },
  "engines": {
    "coc": ">=0.0.23"
  },
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/narajaon/coc-script.git"
  },
  "keywords": [
    "coc.nvim",
    "npm"
  ],
  "author": "narajaon",
  "license": "MIT",
  "bugs": {
    "url": "git+https://github.com/narajaon/coc-script/issues"
  },
  "homepage": "https://github.com/neoclide/coc-jest#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Script",
      "properties": {}
    },
    "commands": [
      {
        "title": "Run script",
        "category": "Script",
        "command": "Script.run"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@types/node": "^13.11.1",
    "@types/which": "^1.3.2",
    "coc.nvim": "^0.0.77",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "eslint": "^6.1.1",
    "typescript": "^3.8.3",
    "which": "^2.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}
